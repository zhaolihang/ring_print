{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "dom",
            "es5",
            "es2015.promise",
            "es6"
        ],
        "target": "es5",
        "sourceMap": true,
        "allowJs": true,
        "listFiles": true,
        "strictNullChecks": false,
        "noEmitHelpers": false
    },
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}